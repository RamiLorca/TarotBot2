{"ast":null,"code":"import UserPrompts from '../components/UserPrompts.vue';\nimport EnterQuery from '../components/EnterQuery.vue';\nimport TarotCards from '../components/TarotCards.vue';\nimport ChosenCards from '../components/ChosenCards.vue';\nexport default {\n  name: 'HomePage',\n  components: {\n    UserPrompts,\n    TarotCards,\n    EnterQuery,\n    ChosenCards\n  },\n  computed: {\n    isEnterQueryVisible() {\n      return this.$store.state.isEnterQueryVisible;\n    },\n    isTarotCardsVisible() {\n      return this.$store.state.isTarotCardsVisible;\n    },\n    showChosenCards() {\n      return this.$store.state.selectedCardsCount === 3;\n    },\n    selectedCards() {\n      return this.$store.getters.selectedCards;\n    }\n  },\n  methods: {\n    incrementProgressBar(payload) {\n      if (payload === true) {\n        this.$refs.UserPrompts.incrementProgressBar();\n      }\n    },\n    showChosenCards() {\n      this.$store.commit('showChosenCards');\n    }\n  }\n};","map":{"version":3,"names":["UserPrompts","EnterQuery","TarotCards","ChosenCards","name","components","computed","isEnterQueryVisible","$store","state","isTarotCardsVisible","showChosenCards","selectedCardsCount","selectedCards","getters","methods","incrementProgressBar","payload","$refs","commit"],"sources":["src/views/HomePage.vue"],"sourcesContent":["<template>\n  <div class=\"main\" style=\"height: 100%;\">\n    <UserPrompts id=\"main-sub-1\" ref=\"UserPrompts\" />\n    <EnterQuery id=\"main-sub-2\" v-if=\"isEnterQueryVisible\" @submit-query=\"incrementProgressBar\"/>\n    <transition name=\"fade-in\">\n      <TarotCards v-if=\"isTarotCardsVisible\" :isVisible=\"isTarotCardsVisible\" @all-cards-selected=\"hideTarotCards\"/>\n    </transition>\n    <ChosenCards v-if=\"showChosenCards\" :selectedCards=\"selectedCards\"/>\n  </div>\n</template>\n\n<script>\nimport UserPrompts from '../components/UserPrompts.vue'\nimport EnterQuery from '../components/EnterQuery.vue'\nimport TarotCards from '../components/TarotCards.vue'\nimport ChosenCards from '../components/ChosenCards.vue'\n\nexport default {\n    name: 'HomePage',\n    components: {\n        UserPrompts,\n        TarotCards,\n        EnterQuery,\n        ChosenCards,\n    },\n    computed: {\n      isEnterQueryVisible() {\n        return this.$store.state.isEnterQueryVisible;\n      },\n      isTarotCardsVisible() {\n        return this.$store.state.isTarotCardsVisible;\n      },\n      showChosenCards() {\n        return this.$store.state.selectedCardsCount === 3;\n      },\n      selectedCards() {\n        return this.$store.getters.selectedCards;\n      },\n    },\n    methods: {\n      incrementProgressBar(payload) {\n        if(payload === true) {\n          this.$refs.UserPrompts.incrementProgressBar();\n        }\n      },\n      showChosenCards() {\n        this.$store.commit('showChosenCards');\n      },\n    },\n}\n</script>\n\n<style scoped>\n\n.main {\n  height: 100%;\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  align-items: center;\n}\n\n.hidden {\n  display: none !important;\n}\n\n.fade-in-enter-active, .fade-in-leave-active {\n  transition: opacity 3s;\n}\n\n.fade-in-enter, .fade-in-leave-to /* .fade-in-leave-active in <2.1.8 */ {\n  opacity: 0;\n}\n\n</style>"],"mappings":"AAYA,OAAAA,WAAA;AACA,OAAAC,UAAA;AACA,OAAAC,UAAA;AACA,OAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAL,WAAA;IACAE,UAAA;IACAD,UAAA;IACAE;EACA;EACAG,QAAA;IACAC,oBAAA;MACA,YAAAC,MAAA,CAAAC,KAAA,CAAAF,mBAAA;IACA;IACAG,oBAAA;MACA,YAAAF,MAAA,CAAAC,KAAA,CAAAC,mBAAA;IACA;IACAC,gBAAA;MACA,YAAAH,MAAA,CAAAC,KAAA,CAAAG,kBAAA;IACA;IACAC,cAAA;MACA,YAAAL,MAAA,CAAAM,OAAA,CAAAD,aAAA;IACA;EACA;EACAE,OAAA;IACAC,qBAAAC,OAAA;MACA,IAAAA,OAAA;QACA,KAAAC,KAAA,CAAAlB,WAAA,CAAAgB,oBAAA;MACA;IACA;IACAL,gBAAA;MACA,KAAAH,MAAA,CAAAW,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}